{
  "name": "node-api-objection",
  "version": "1.0.0",
  "description": "Simple list api using Typescript with Espress and ObjectionJS",
  "main": "src/server.ts",
  "repository": "https://github.com/luanglopes/node-api-objection.git",
  "author": "Luan Lopes",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev --inspect --transpileOnly --ignore-watch node_modules src/server.ts",
